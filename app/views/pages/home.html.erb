<h1>My AC Mideterm Assessment</h1>

<h3><%= link_to "create doctor", new_doctor_path %></h3>

<table>
  <thead>
    LIST OF DOCTORS
  </thead>
  <tbody>
    <% Doctor.all.each do |doctor| %>
      <tr>
        <td><%= doctor.username %></td>
        <td><%= doctor.address %></td>
        <td><%= doctor.gender %></td>
        <td><%= link_to 'Show', doctor_path(doctor) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
